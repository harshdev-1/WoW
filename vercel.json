{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    },
    {
      "src": "routes/userRoutes.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/user/(.*)",
      "dest": "/routes/userRoutes.js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
