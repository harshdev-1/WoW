{
    "version": 2,
    "builds": [
      {
        "src": "./index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./index.js"
      },
      {
        "src": "/login(.*)",
        "dest": "./index.js"
      },
      {
        "src": "/user/signup(.*)",
        "dest": "./index.js"
      },
      {
        "src": "/user/login(.*)",
        "dest": "./index.js"
      },
      {
        "src": "/user/profile(.*)",
        "dest": "./index.js"
      }
    ]
  }
  